 {{#each savedArticle}}
<div class="row">
    <div class="col s12 m12">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
                <span class="card-title" class="title">{{title}}</span>
                <p class="summary">{{summary}}</p>
                <a href="{{link}}" class="link">{{link}}</a>
            </div>
            <div class="card-action">
                <a href="#" class="waves-effect waves-light btn">delete article</a>
                <a href="#" class="waves-effect waves-light btn">add note</a>
            </div>
        </div>
    </div>
</div>
{{/each}}